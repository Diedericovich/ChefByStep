@model RecipeViewModel

<div class="overview">
    <h2>All recipes</h2>
    <div class="row">
        @foreach (var recipe in Model.Recipes)
        {
            <div class="col-sm-12 col-md-6 col-lg-3 ">
                <a class="nav-link" asp-area="" asp-controller="Recipe" asp-action="Detail" asp-route-id="@recipe.Id">
                    <img class="recipe-img" src="@recipe.ImageUrl" />

                    <div class="centered">@recipe.Title</div>
                </a>
            </div>
        }
    </div>
</div>

